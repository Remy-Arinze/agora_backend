{
  "name": "agora-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "prisma generate && nest build",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "start": "nest start",
    "start:prod": "node dist/main",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name migration --skip-generate",
    "db:migrate:create": "prisma migrate dev --create-only",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed-nerdc": "tsx prisma/seed-nerdc.ts",
    "db:check-failed-migration": "tsx scripts/check-failed-migration.ts",
    "db:resolve-failed-migration": "tsx scripts/check-failed-migration.ts --resolve",
    "db:apply-missing-class-arms": "prisma db execute --file scripts/apply-missing-class-arms.sql"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^5.1.1",
    "@prisma/client": "^5.9.1",
    "@types/nodemailer": "^7.0.4",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.8.0",
    "cookie-parser": "^1.4.6",
    "decimal.js": "^10.6.0",
    "googleapis": "^166.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.3.11",
    "nodemailer": "^7.0.10",
    "openai": "^6.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "readable-stream": "^4.7.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.5",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^5.9.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": ["**/*.(t|j)s"],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
